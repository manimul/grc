    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-source-filesystem": "^2.3.29",
    "gatsby-source-wordpress": "^4.0.4",
    "gatsby-transformer-sharp": "^2.5.14",
    "headroom.js": "^0.11.0",
    "intersection-observer": "^0.11.0",
    "prop-types": "15.7.2",
